 													       </c:when>                                                                                                                                                                                            
 													        <c:when test="${ac.type==3}">                                                                                                                                                                    
 													             聊天                                                                                                                                                                                         
 													       </c:when>   
 													        <c:when test="${ac.type==4}">                                                                                                                                                                    
 													             分享                                                                                                                                                                                        
 													       </c:when>  
 													        <c:when test="${ac.type==5}">                                                                                                                                                                    
