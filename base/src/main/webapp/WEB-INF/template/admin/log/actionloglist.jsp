 													       </c:when>   
 													        <c:when test="${ac.shareType==8}">                                                                                                                                                                    
 													            系统分享                                                                                                                                                                                        
 													       </c:when>                                                                                                                                                                                       
 													       <c:otherwise>                                                                                                                                                                                        
 													              未知                                                                                                                                                                                         
 													       </c:otherwise>                                                                                                                                                                                       
 												</c:choose>               
 											
 											</td>                                                                                                                                                                          
